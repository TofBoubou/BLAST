<gasp2 surface_model="catalysis">
    <reactions model="finite_rate">
        <site_density>7.5e-6</site_density>
        <reaction type="Adsorption" formula="N + (s) -> N(s)">
                <parameters>S0:0.1,beta:0.0,Ead:0.0</parameters>
        </reaction>
        <reaction type="Desorption" formula="N(s) -> N + (s)">
                <parameters>Ades:1.0,beta:0.0, v:1.0,Edes:0.0</parameters>
        </reaction>
        <reaction type="Eley-Rideal" formula="N2 + (s) <-> N + N(s)">
                <parameters>gamma_er:0.001,beta:0.0,Eer:9000.0</parameters>
        </reaction>
    </reactions>
</gasp2>
