<?xml version="1.0" encoding="UTF-8"?>
<gasp2 surface_model="catalysis">
    <!-- CO2_5 species: CO2, CO, O2, O, C -->
    
    <reactions model="finite_rate">
        <site_density>7.5e-6</site_density>
        
        <!-- Adsorption des espèces réactives avec paramètres validés -->
        <reaction type="Adsorption" formula="O + (s) -> O(s)">
            <parameters>S0:0.5,beta:0.0,Ead:0.0</parameters>
        </reaction>
        <reaction type="Adsorption" formula="CO + (s) -> CO(s)">
            <parameters>S0:0.3,beta:0.0,Ead:0.0</parameters>
        </reaction>
        
        <!-- Désorption des espèces réactives -->
        <reaction type="Desorption" formula="O(s) -> O + (s)">
            <parameters>Ades:1.0,beta:0.0,v:1.0,Edes:0.0</parameters>
        </reaction>
        <reaction type="Desorption" formula="CO(s) -> CO + (s)">
            <parameters>Ades:1.0,beta:0.0,v:1.0,Edes:0.0</parameters>
        </reaction>
        
        <!-- Réactions Eley-Rideal pour tes deux réactions cibles (reversible required) -->
        <reaction type="Eley-Rideal" formula="O + O(s) <-> O2 + (s)">
            <parameters>gamma_er:0.2,beta:0.0,Eer:0.0</parameters>
        </reaction>
        <reaction type="Eley-Rideal" formula="CO + O(s) <-> CO2 + (s)">
            <parameters>gamma_er:0.2,beta:0.0,Eer:0.0</parameters>
        </reaction>
        
    </reactions>
</gasp2>