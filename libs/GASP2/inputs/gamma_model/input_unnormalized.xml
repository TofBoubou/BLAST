<gasp2 surface_model="catalysis">
    <reactions model="gamma_model">
        <reaction type="GammaGiven" formula="2N + 2N -> 2N2">
                <gammas>N:0.5</gammas>
        </reaction>
        <reaction type="GammaGiven" formula="2O + 2O -> 2O2">
                <gammas>O:0.5</gammas>
        </reaction>
        <reaction type="GammaGiven" formula="N + O -> NO">
                <gammas>N:0.5, O:0.5</gammas>
        </reaction>
    </reactions>
</gasp2>
