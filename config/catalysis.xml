<?xml version="1.0" encoding="UTF-8"?>
<gasp2_catalysis>
  <!-- CO2_5 species: CO2, CO, O2, O, C -->
  
  <surface_reactions>
    
    <!-- Finite rate catalysis reactions for CO2/air mixture -->
    
    <!-- O atom recombination -->
    <reaction id="1">
      <equation>O + surface -> O_ads</equation>
      <type>finite_rate</type>
      <activation_energy units="J/mol">0.0</activation_energy>
      <pre_exponential units="m2/mol/s">1.0e13</pre_exponential>
      <temperature_exponent>0.0</temperature_exponent>
    </reaction>
    
    <reaction id="2">
      <equation>O_ads + O -> O2 + surface</equation>
      <type>finite_rate</type>
      <activation_energy units="J/mol">0.0</activation_energy>
      <pre_exponential units="m2/mol/s">1.0e13</pre_exponential>
      <temperature_exponent>0.0</temperature_exponent>
    </reaction>
    
    <!-- CO oxidation -->
    <reaction id="3">
      <equation>CO + surface -> CO_ads</equation>
      <type>finite_rate</type>
      <activation_energy units="J/mol">0.0</activation_energy>
      <pre_exponential units="m2/mol/s">1.0e13</pre_exponential>
      <temperature_exponent>0.0</temperature_exponent>
    </reaction>
    
    <reaction id="4">
      <equation>CO_ads + O -> CO2 + surface</equation>
      <type>finite_rate</type>
      <activation_energy units="J/mol">0.0</activation_energy>
      <pre_exponential units="m2/mol/s">1.0e13</pre_exponential>
      <temperature_exponent>0.0</temperature_exponent>
    </reaction>
    
    <!-- C atom oxidation -->
    <reaction id="5">
      <equation>C + surface -> C_ads</equation>
      <type>finite_rate</type>
      <activation_energy units="J/mol">0.0</activation_energy>
      <pre_exponential units="m2/mol/s">1.0e13</pre_exponential>
      <temperature_exponent>0.0</temperature_exponent>
    </reaction>
    
    <reaction id="6">
      <equation>C_ads + O2 -> CO2 + surface</equation>
      <type>finite_rate</type>
      <activation_energy units="J/mol">0.0</activation_energy>
      <pre_exponential units="m2/mol/s">1.0e13</pre_exponential>
      <temperature_exponent>0.0</temperature_exponent>
    </reaction>
    
  </surface_reactions>
  
  <!-- Species mapping -->
  <species_mapping>
    <species name="CO2" index="0"/>
    <species name="CO" index="1"/>
    <species name="O2" index="2"/>
    <species name="O" index="3"/>
    <species name="C" index="4"/>
  </species_mapping>
  
  <!-- Surface species (adsorbed species) -->
  <surface_species>
    <species name="O_ads"/>
    <species name="CO_ads"/>
    <species name="C_ads"/>
  </surface_species>
  
</gasp2_catalysis>